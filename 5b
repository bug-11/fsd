// Use database
use studentDB;

// INSERT
db.students.insert({ roll: 1, name: "Likki", marks: 95 });
db.students.insert({ roll: 2, name: "Joe", marks: 88 });

// FIND
db.students.find();                // All documents
db.students.find({ roll: 1 });     // Specific document

// UPDATE
db.students.update(
  { roll: 2 },
  { $set: { marks: 90 } }
);

// DELETE
db.students.remove({ roll: 1 });
